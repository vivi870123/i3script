{
	"coc.preferences.bracketEnterImprove": true,
	"coc.preferences.colorSupport": false,
	"coc.preferences.currentFunctionSymbolAutoUpdate": false,
	"coc.preferences.enableFloatHighlight": true,
	"coc.preferences.formatOnSaveFiletypes": [
		/* "css", */
		"markdown",
		"javascript",
		"typescript",
		"typescriptreact"
	],

	"coc.preferences.formatOnType": false,
	"coc.preferences.hoverTarget": "float",
	"coc.preferences.jumpCommand": "edit",
	"coc.preferences.messageLevel": "more",
	"coc.preferences.previewAutoClose": true,
	"coc.preferences.snippetStatusText": "Ⓢ ",
	"coc.preferences.snippets.enable": true,
	"coc.preferences.useQuickfixForLocations": false,
	"coc.source.around.enable": true,
	"coc.source.around.disableSyntaxes": [],
	"coc.source.around.firstMatch": true,
	"coc.source.around.priority": 1,
	"coc.source.around.shortcut": "A",
	"coc.source.neco.enable": true,
	"coc.source.neco.shortcut": "NEC",
	"coc.source.buffer.enable": true,
	"coc.source.buffer.disableSyntaxes": [],
	"coc.source.buffer.firstMatch": true,
	"coc.source.buffer.ignoreGitignore": false,
	"coc.source.buffer.priority": 1,
	"coc.source.buffer.shortcut": "B",
	"coc.source.file.enable": true,
	"coc.source.file.disableSyntaxes": [],
	"coc.source.file.ignoreHidden": true,
	"coc.source.file.ignorePatterns": ["**/dist/*"],
	"coc.source.file.priority": 99,
	"coc.source.file.shortcut": "F",
	"coc.source.file.triggerCharacters": ["/"],
	"coc.source.file.trimSameExts": [".ts", ".js"],
	"coc.source.word.enable": true,
	"coc.source.word.filetypes": ["markdown", "text"],

	"codeLens.enable": true,
	"codeLens.separator": "",

	"diagnostic.checkCurrentLine": false,
	"diagnostic.displayByAle": false,
	"diagnostic.enable": true,
	"diagnostic.enableMessage": "never",
	"diagnostic.joinMessageLines": false,
	"diagnostic.locationlist": true,
	"diagnostic.messageTarget": "echo",
	"diagnostic.maxWindowHeight": 5,
	"diagnostic.refreshAfterSave": false,
	"diagnostic.refreshOnInsertMode": true,
	"diagnostic.virtualText": false,
	"diagnostic.virtualTextLines": 3,
	"diagnostic.virtualTextLineSeparator": " \\ ",
	"diagnostic.virtualTextPrefix": "  ",
	"diagnostic.highlightOffset": 1000,
	"diagnostic.signOffset": 999999,
	"diagnostic.enableSign": true,
	"diagnostic.errorSign": "•",
	"diagnostic.hintSign": "•",
	"diagnostic.infoSign": "•",
	"diagnostic.warningSign": "•",

	"diagnostic-languageserver.enable": true,
	"diagnostic-languageserver.filetypes": {
		"email": "languagetool",
		"markdown": ["write-good", "alex"],
		"text": ["write-good", "alex"],
		"vim": "vint"},

	"emmet.excludeLanguages": ["markdown", "php"],
	"emmet.includeLanguages": {
		"jinja.html": "html",
		"javascript": "javascriptreact",
		"javascript.jsx": "javascriptreact"},
	"emmet.showAbbreviationSuggestions": false,
	"emmet.showExpandedAbbreviation": "always",
	"emmet.showSuggestionsAsSnippets": false,

	"eslint.filetypes": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"],

	"eslint.autoFixOnSave": false,
	"eslint.packageManager": "yarn",

  "explorer.file.column.git.showIgnored": true,
	"explorer.file.column.indent.indentLine": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
	"explorer.icon.hidden": "I",
	"explorer.icon.expanded": "-",
	"explorer.icon.selected": "✓",
	"explorer.icon.collapsed": "+",
	"explorer.icon.enableVimDevions": true,
	"explorer.position": "right",
	"explorer.sources": [{"name": "file", "expand": true}],
  "explorer.quitOnOpen": true,
	"explorer.width": 35,
	"explorer.file.columns": [
		"selection",
		"clip",
		"diagnosticError",
		"indent",
		"icon",
		"filename",
		"size",
		"modified",
		"readonly"
	],

	"explorer.file.column.git.icon.added": "✚",
	"explorer.file.column.git.icon.deleted": "✖",
	"explorer.file.column.git.icon.renamed": "≫",
	"explorer.file.column.git.icon.unmerged": "≠",
	"explorer.file.column.git.icon.modified": "•",
	"explorer.file.column.git.icon.untracked": "ᵁ",
	"explorer.file.column.git.icon.ignored": "ⁱ",

	"explorer.keyMappingMode": "none",
	"explorer.keyMappings": {
    "k": "nodePrev",
    "j": "nodeNext",

    "<space>": ["toggleSelection", "normal:j"],
    "<tab>": "actionMenu",

    "h": "collapse",
    "l": "expand",
    "J": ["toggleSelection", "normal:j"],
    "K": ["toggleSelection", "normal:k"],
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "<cr>": "open",
    "o": "openOrCollapse",
    "e": "open",
    "sg": "openInVsplit",
    "st": "openInTab",
    "u": "gotoParent",
    "<bs>": "gotoParent",

    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",

    "n": "addFile",
    "N": "addDirectory",
    "r": "rename",

    ".": "toggleHidden",
    "R": "refresh",

    "?": "help",
    "q": "quit",
    "X": "systemExecute",
    "gd": "listDrive",

    "f": "search",
    "F": "searchRecursive",

    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",

    "[[": "sourcePrev",
    "]]": "sourceNext",

    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",

    "[c": "gitPrev",
    "]c": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
	},

	"git.enableGutters": true,
	"git.branchCharacter": "4",
	"git.addGBlameToBufferVar": true,
	"git.addGBlameToVirtualText": false,
	"git.virtualTextPrefix": " ❯❯❯ ",
	"git.addedSign.hlGroup": "GitGutterAdd",
	"git.changedSign.hlGroup": "GitGutterChange",
	"git.removedSign.hlGroup": "GitGutterDelete",
	"git.topRemovedSign.hlGroup": "GitGutterDelete",
	"git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
	"git.addedSign.text": "▎",
	"git.changedSign.text": "▎",
	"git.removedSign.text": "▏",
	"git.topRemovedSign.text": "▔",
	"git.changeRemovedSign.text": "▋",

	"intelephense.format.enable": true,
	"intelephense.files.exclude": [
		"**/.git/**",
		"**/.svn/**",
		"**/.hg/**",
		"**/CVS/**",
		"**/.DS_Store/**",
		"**/node_modules/**",
		"**/bower_components/**",
		"**/vendor/**/{Test,test,Tests,tests}/**"
	],
	"intelephense.stubs": [
		"com_dotnet",
		"Core",
		"date",
		"filter",
		"gd",
		"mbstring",
		"mssql",
		"mysqli",
		"pcre",
		"Phar",
		"regex",
		"SPL",
		"standard",
		"xml",
	],

  "javascript.implementationsCodeLens.enable": false,
  "javascript.referencesCodeLens.enable": false,
	"javascript.suggest.autoImports": true,
	"javascript.suggest.completeFunctionCalls": true,
	"javascript.suggest.enabled": true,
	"javascript.suggest.paths": true,
	"javascript.suggestionActions.enabled": true,
	"javascript.validate.enable": false,

	"json.format.enable": true,

	"languageserver": {
		"intelephense": {
			"command": "intelephense",
			"args": ["--stdio"],
			"filetypes": ["php"],
			"initializationOptions": {
				"storagePath": "/tmp/intelephense"
			}
		},
		"bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
	},

	"list.nextKeymap": "<C-j>",
	"list.previousKeymap": "<C-k>",

	"list.insertMappings": {
		"<c-c>": "do:exit",
		"<c-x>": "do:stop",
		"<c-t>": "action:tabe",
		"<c-g>": "action:vsplit",
		"<c-v>": "action:split",
	},

	"list.autoResize": true,
	"list.extendedSearchMode": true,
	"list.indicator": ">",
	"list.limitLines": 30000,
	"list.maxHeight": 10,
	"list.maxPreviewHeight": 12,
	"list.previewHighlightGroup": "Search",
	"list.previewSplitRight": false,
	"list.selectedSignText": "*",
	"list.signOffset": 900,
	"list.source.files.excludePatterns": [
		"/dist/*",
		"**/vendor/**",
		"**/node_modules/**",
		"**/bower_components/**",
		"**/.git/**",
	],
	"list.source.mru.excludePatterns": ["../.git/*", "/tmp/*", ".gitignore"],

	"markdownlint.config": {
		"default": true,
	},

	"pairs.enableCharacters": [ "(", "[", "{", "'", "\"", "`" ],

	"phpls.enable": false,

	"prettier.disableSuccessMessage": true,
	"prettier.onlyUseLocalVersion": false,
	"prettier.singleQuote": true,
	"prettier.semi": true,
	"prettier.statusItemText": "ⓟ ",
	"prettier.tslintIntegration": true,
	"prettier.trailingComma": "es5",

	"python.jediEnabled": false,

	"session.directory": "~/.cache/nvim/session",

	"signature.enable": true,
	"signature.hideOnTextChange": false,
	"signature.maxWindowHeight": 8,
	"signature.preferShownAbove": true,
	"signature.target": "echo",
	"signature.triggerSignatureWait": 50,

	"smartf.timeout": 2000,
	"smartf.jumpOnTrigger": true,

	"snippets.extends": {
		"javascriptreact": ["javascript"],
		"typescriptreact": ["typescript"]
	},
	"snippets.priority": -10,
	"snippets.shortcut": "S",
	"snippets.autoTrigger": true,
	"snippets.triggerCharacters": [
		".", "t"
	],

	"suggest.autoTrigger": "always",

	"suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\ue79b",
		"value": "\uf89f",
		"operator": "\u03a8",
		"function": "\u0192",
		"reference": "\ufa46",
		"constant": "\uf8fe",
		"method": "\uf09a",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\uf417",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\uf40d",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf9be",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\ue60b",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},

	"suggest.detailField": "menu",
	"suggest.detailMaxLength": 30,
	"suggest.disableKind": true,
	"suggest.disableMenu": false,
	"suggest.disableMenuShortcut": false,
	"suggest.echodocSupport": true,
	"suggest.enablePreselect": false,
	"suggest.enablePreview": false,
	"suggest.fixInsertedWord": true,
	"suggest.floatEnable": true,
	"suggest.keepCompleteopt": true,
	"suggest.labelMaxLength": 50,
	"suggest.languageSourcePriority": 99,
	"suggest.localityBonus": true,
	"suggest.maxCompleteItemCount": 30,
	"suggest.maxPreviewWidth": 50,
	"suggest.minTriggerInputLength": 2,
	"suggest.noselect": false,
	"suggest.numberSelect": false,
	"suggest.preferCompleteThanJumpPlaceholder": false,
	"suggest.removeDuplicateItems": false,
	"suggest.snippetIndicator": "⭐︎",
	"suggest.timeout": 4000,
	"suggest.triggerAfterInsertEnter": false,
	"suggest.triggerCompletionWait": 100,

	"todolist.enable": true,
	"todolist.autoUpload": false,
	"todolist.maxsize": 5000,
	"todolist.monitor": true,
	"todolist.reminder.notify": "floating",
	"todolist.reminder.winblend": 2,

	"tsserver.enableJavascript": true,
	"tsserver.formatOnType": true,
	"tsserver.locale": "en-gb",

  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
	"typescript.implementationsCodeLens.enable": false,
	"typescript.preferences.importModuleSpecifier": "relative",
	"typescript.preferences.quoteStyle": "double",
	"typescript.referencesCodeLens.enable": false,
	"typescript.suggest.autoImports": true,
	"typescript.suggest.completeFunctionCalls": true,
	"typescript.suggest.enabled": true,
	"typescript.suggest.paths": true,
	"typescript.suggestionActions.enabled": true,

	"typescript.updateImportsOnFileMove.enable": true,
	"typescript.validate.enable": true,

	"vimlsp.debug": true,
	"vimlsp.diagnostic.enable": false,

	"workspace.ignoredFiletypes": ["markdown", "log", "txt", "help"],

	"yank.highlight.enable": true,
	"yank.highlight.duration": 100,
	"yank.enableCompletion": false,
	"yank.list.maxsize": 200
}


